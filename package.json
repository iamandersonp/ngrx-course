{
  "name": "angular-ngrx-course",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --proxy-config ./proxy.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "test": "ng test --no-watch --detect-open-handles",
    "test:watch": "ng test --watch --detect-open-handles",
    "test:coverage": "ng test --no-watch --coverage  --detect-open-handles 2> /dev/null",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:run": "npm run cypress:run",
    "prettier:check": "prettier --check src/**/**/**/**/**/**/**/**/*.{html,ts,scss}",
    "prettier:write": "prettier --write src/**/**/**/**/**/**/**/**/*.{html,ts,scss}",
    "prettier:checke2e": "prettier --check cypress/**/**/**/**/**/**/**/**/*.{html,ts,scss}",
    "prettier:writee2e": "prettier --write cypress/**/**/**/**/**/**/**/**/*.{html,ts}",
    "setup:git-hooks": "git config core.hooksPath .git-hooks || true && chmod +x ./.git-hooks/*",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "delete:reports": "rm cypress/results/* || true",
    "mocka:report": "npx marge mochawesome.json",
    "mocka:delete": "rm -rf mochawesome-report || true && rm -rf mochawesome.json || true",
    "delete:screenshots": "rm -rf cypress/screenshots/* || true",
    "combine:reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "prereport": "npm run delete:reports && npm run delete:screenshots && npm run mocka:delete",
    "report": "npx cypress run --reporter mochawesome  --reporter-options reportDir=\"cypress/results\",overwrite=false,html=false,json=true",
    "postreport": "npm run combine:reports && npm run mocka:report",
    "postinstall": "npm run setup:git-hooks"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^17.0.5",
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/material": "^17.0.2",
    "@angular/material-moment-adapter": "^17.0.2",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@ngrx/data": "^17.0.1",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/entity": "^17.0.1",
    "@ngrx/router-store": "^17.0.1",
    "@ngrx/store": "^17.0.1",
    "@ngrx/store-devtools": "^17.0.1",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "moment": "^2.22.2",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "17.0.2",
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/language-service": "^17.0.5",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@cypress/schematic": "^2.5.1",
    "@ngrx/eslint-plugin": "^17.0.1",
    "@ngrx/schematics": "^17.0.1",
    "@types/express": "^4.0.39",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "29.5.12",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "cypress": "latest",
    "cypress-multi-reporters": "^1.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "junit-report-merger": "^6.0.3",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "ngx-spec": "^2.1.6",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "standard-version": "^9.5.0",
    "ts-node": "~3.2.0",
    "typescript": "~5.2.2"
  }
}
